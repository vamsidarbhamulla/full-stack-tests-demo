{
  "name": "web-tests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --config eslint.config.mjs --fix && eslint tests --config eslint.config.mjs --fix",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "test": "npx playwright test",
    "test:chromium": "CREATE_NEW_USER=true npx playwright test --project=chromium",
    "test:chromium:ci": "CI=true CREATE_NEW_USER=true npx playwright test checkout.spec --project=chromium",
    "report": "playwright show-report playwright-report",
    "codegen": "npx playwright codegen http://localhost:3000",
    "test-p": "npx playwright test --project=chromium --headed reviewProduct.spec --trace on",
    "test-v": "npx playwright test --project=chromium --headed viewProducts.spec --trace on",
    "test-r": "npx playwright test --project=chromium --headed registerUser.spec --trace on",
    "test-a": "npx playwright test --project=chromium --headed newUserLogin.spec --trace on",
    "test-c": "CREATE_NEW_USER=true npx playwright test --project=chromium --headed checkout.spec --trace on",
    "test-d": "PWDEBUG=console CREATE_NEW_USER=true npx playwright test --project=chromium --headed checkout.spec --trace on"
  },
  "keywords": [],
  "author": "Vamsi Darbhamulla",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@ngneat/falso": "7.2.0",
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "tslib": "^2.8.1",
    "typescript": "5.7.2",
    "winston": "^3.17.0"
  }
}
