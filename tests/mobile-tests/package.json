{
    "name": "appium-boilerplate",
    "version": "5.3.1",
    "author": "Vamsi Krishna Darbhamulla <vamsi.krishna@gmail.com>",
    "license": "MIT",
    "description": "Boilerplate project to run WebdriverIO tests with Appium to test native applications on iOS and Android",
    "engines": {
        "node": "^16.13.0 || >=18.0.0"
    },
    "keywords": [
        "WebdriverIO",
        "WebDriver",
        "appium",
        "native app",
        "android"
    ],
    "type": "module",
    "scripts": {
        "android.app": "wdio run config/wdio.android.app.conf.ts",
        "lint": "eslint ./**/*.ts --config eslint.config.mjs --fix",
        "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
        "allure-report": "allure generate --clean allure-results",
        "allure-open": "allure open allure-report",
        "allure-open-ci": "allure open reports/mobile-tests-report"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^2.12.1",
        "@typescript-eslint/eslint-plugin": "^8.2.0",
        "@typescript-eslint/parser": "^8.2.0",
        "eslint-plugin-wdio": "^9.0.5",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.10.2",
        "eslint": "^8.57.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^52.0.0",
        "@wdio/allure-reporter": "^9.4.4",
        "@wdio/appium-service": "^9.0.7",
        "@wdio/browserstack-service": "^9.0.7",
        "@wdio/cli": "^9.0.7",
        "@wdio/cucumber-framework": "^9.0.6",
        "@wdio/dot-reporter": "^9.4.4",
        "@wdio/globals": "^9.0.7",
        "@wdio/local-runner": "^9.0.7",
        "@wdio/mocha-framework": "^9.0.6",
        "@wdio/spec-reporter": "^9.0.7",
        "allure-commandline": "^2.13.0",
        "appium": "^2.11.3",
        "appium-installer": "3.0.0",
        "appium-uiautomator2-driver": "^3.7.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4"
    }
}
